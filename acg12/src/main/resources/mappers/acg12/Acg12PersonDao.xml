	</sql>

	<!-- 根据ID查询 -->
	<select id="find" resultMap="Acg12PersonResultMap">
		SELECT	<include refid="base_column" />
		FROM	T_ACG12_PERSON
		WHERE	ID=#{id,jdbcType=BIGINT}
	</select>
	
	<!-- 查询所有 -->
	<select id="findAll" resultMap="Acg12PersonResultMap">
