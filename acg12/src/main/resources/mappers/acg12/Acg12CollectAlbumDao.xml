		</if>
	</sql>

	<!-- 根据ID查询 -->
	<select id="find" resultMap="Acg12CollectAlbumResultMap">
		SELECT	<include refid="base_column" />
		FROM	T_ACG12_COLLECT_ALBUM
		WHERE	ID=#{id,jdbcType=BIGINT}
	</select>
	
