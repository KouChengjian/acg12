<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.acg12.dao.TagDao">

    <resultMap id="BaseResultMap" type="com.acg12.entity.po.Tag">
        <result column="tagId" property="tagId" jdbcType="INTEGER"/>
        <result column="tag" property="tag" jdbcType="VARCHAR"/>
        <result column="createTime" property="createTime" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="insert" parameterType="com.acg12.entity.po.Tag" useGeneratedKeys="true" keyProperty="tagId">
        insert into acg_tag (tagId , tag , createTime)
        values (#{tagId,jdbcType=INTEGER},#{tag,jdbcType=VARCHAR}, #{createTime,jdbcType=INTEGER})
    </insert>

    <select id="queryList" resultType="com.acg12.entity.po.Tag">
        select * from acg_tag
    </select>

</mapper>