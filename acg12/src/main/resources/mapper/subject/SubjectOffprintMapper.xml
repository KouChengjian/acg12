<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.acg12.dao.subject.SubjectOffprintDao">

    <resultMap id="subjectOffprintResultMap" type="com.acg12.entity.po.subject.SubjectOffprintEntity">
        <result column="subjectOffprintId" property="subjectSongId" jdbcType="INTEGER"/>
        <result column="subjectId" property="subjectId" jdbcType="INTEGER"/>
        <result column="sId" property="sId" jdbcType="INTEGER"/>
        <result column="id" property="id" jdbcType="INTEGER"/>
        <result column="image" property="title" jdbcType="VARCHAR"/>
        <result column="name" property="title" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="insert" parameterType="com.acg12.entity.po.subject.SubjectOffprintEntity" useGeneratedKeys="true" keyProperty="subjectOffprintId">
        insert into acg_subject_offprint (subjectOffprintId, subjectId, sId, id, image, name)
        values (#{subjectOffprintId,jdbcType=INTEGER},#{subjectId,jdbcType=INTEGER},#{sId,jdbcType=INTEGER}, #{id,jdbcType=INTEGER},#{image,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR})
    </insert>

    <insert id="insertList" parameterType="java.util.List" >
        insert into acg_subject_offprint (subjectOffprintId, subjectId, sId, id, image, name)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.subjectOffprintId,jdbcType=INTEGER}, #{item.subjectId,jdbcType=INTEGER}, #{item.sId,jdbcType=INTEGER}, #{item.id,jdbcType=INTEGER}
            ,#{item.image,jdbcType=VARCHAR}) ,#{item.name,jdbcType=VARCHAR})
        </foreach>
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="subjectOffprintId">
            SELECT LAST_INSERT_ID();
        </selectKey>
    </insert>

    <select id="queryBySubjectOffprintId" parameterType="int" resultType="com.acg12.entity.po.subject.SubjectOffprintEntity">
        select * from acg_subject_offprint where subjectOffprintId = #{subjectOffprintId}
    </select>

    <select id="queryByOffprint" parameterType="int" resultType="com.acg12.entity.po.subject.SubjectOffprintEntity">
        select * from acg_subject_offprint where sId = #{sId} and id = #{id}
    </select>

    <select id="queryBySubjectId" parameterType="int" resultType="com.acg12.entity.po.subject.SubjectOffprintEntity">
        select * from acg_subject_offprint where subjectId = #{subjectId}
    </select>

    <select id="queryBySId" parameterType="int" resultType="com.acg12.entity.po.subject.SubjectOffprintEntity">
        select * from acg_subject_offprint where sId = #{sId}
    </select>
</mapper>