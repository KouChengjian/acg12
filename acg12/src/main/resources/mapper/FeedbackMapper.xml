<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.acg12.mapper.FeedbackMapper" >

    <resultMap id="BaseResultMap" type="com.acg12.beans.Feedback" >
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="uid" property="phone" jdbcType="INTEGER" />
        <result column="message" property="verifycode" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="insert" parameterType="com.acg12.beans.Feedback"
            useGeneratedKeys="true" keyProperty="id">
        insert into feedback (id , uid , message)
        values (#{id,jdbcType=INTEGER},#{uid,jdbcType=INTEGER}, #{message,jdbcType=VARCHAR})
    </insert>

