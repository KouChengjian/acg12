<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.acg12.dao.BangumiDao" >

    <resultMap id="BaseResultMap" type="com.acg12.entity.po.Bangumi" >
        <result column="objectId" property="objectId" jdbcType="INTEGER" />
        <result column="platform" property="platform" jdbcType="VARCHAR" />
        <result column="title" property="title" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="insert" parameterType="com.acg12.entity.po.Bangumi" useGeneratedKeys="true" keyProperty="objectId">
        insert into acg_bangumi (objectId , platform , title)
        values (#{objectId,jdbcType=INTEGER},#{platform,jdbcType=VARCHAR}, #{title,jdbcType=VARCHAR})
    </insert>

    <insert id="insertList" parameterType="java.util.List" useGeneratedKeys="true" >
        <selectKey resultType="INTEGER" keyProperty="objectId" order="AFTER">
            SELECT
            LAST_INSERT_ID()
        </selectKey>
        insert into acg_bangumi (platform , title)
        values
        <foreach collection="list" item="item" index="index" separator="," >
            (#{item.platform},#{item.title})
        </foreach>
    </insert>


