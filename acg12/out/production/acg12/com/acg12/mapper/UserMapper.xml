<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.acg12.mapper.UserMapper" >

    <resultMap id="BaseResultMap" type="com.acg12.beans.User" >
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="username" property="username" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="email" property="email" jdbcType="VARCHAR" />
        <result column="updated" property="updated" jdbcType="VARCHAR" />
        <result column="created" property="created" jdbcType="VARCHAR" />
    </resultMap>



    <insert id="insert" parameterType="com.acg12.beans.User"
            useGeneratedKeys="true" keyProperty="id">
        insert into user (id,username, password,email,updated,created)
        values (#{id,jdbcType=INTEGER},#{username,jdbcType=VARCHAR}, #{password,jdbcType=VARCHAR}, #{email,jdbcType=VARCHAR}
        , #{updated,jdbcType=VARCHAR}, #{created,jdbcType=VARCHAR})
    </insert>

</mapper>