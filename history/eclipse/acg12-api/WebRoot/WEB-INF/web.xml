<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>/WEB-INF/acg12-servlet.xml</param-value>
	</context-param>

	<servlet>
		<servlet-name>acg12</servlet-name>
		<servlet-class>
			org.springframework.web.servlet.DispatcherServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>acg12</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener
		</listener-class>
	</listener>






	<filter>
		<filter-name>CharactorEncodingFilter</filter-name>
		<filter-class>org.acg12.filter.CharactorEncodingFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>CharactorEncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<!-- 移动请求接口 -->
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>org.acg12.servlet.Home</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeInfo</servlet-name>
		<servlet-class>org.acg12.servlet.HomeInfo</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeMore</servlet-name>
		<servlet-class>org.acg12.servlet.HomeMore</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Find</servlet-name>
		<servlet-class>org.acg12.servlet.Find</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FindInfo</servlet-name>
		<servlet-class>org.acg12.servlet.FindInfo</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FindInfoAv</servlet-name>
		<servlet-class>org.acg12.servlet.FindInfoAv</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Search</servlet-name>
		<servlet-class>org.acg12.servlet.Search</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PlayUrl</servlet-name>
		<servlet-class>org.acg12.servlet.PlayUrl</servlet-class>
	</servlet>







	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeMore</servlet-name>
		<url-pattern>/home/more</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeInfo</servlet-name>
		<url-pattern>/home/info</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Find</servlet-name>
		<url-pattern>/find</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FindInfo</servlet-name>
		<url-pattern>/find/info</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FindInfoAv</servlet-name>
		<url-pattern>/find/v</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PlayUrl</servlet-name>
		<url-pattern>/playurl</url-pattern>
	</servlet-mapping>




</web-app>